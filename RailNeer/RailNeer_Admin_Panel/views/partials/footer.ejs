<script>
    function updatePlantDetail(selectplant_id) {
        var data = JSON.stringify({
            "plant_id": selectplant_id.options[selectplant_id.selectedIndex].value
        });

        $.ajax({
            type: "POST",
            contentType: 'application/json',
            url: '/updatePlantinMain',
            data: data,
            success: function (res) {
                location.reload(true);
                // alert('hey');
            }
        });
    }
</script>
