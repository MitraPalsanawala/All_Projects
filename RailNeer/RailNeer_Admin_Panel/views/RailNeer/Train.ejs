                                                                            <label for="frequency5">Friday</label>


                                                                            <input type="checkbox" id="frequency6"
                                                                                name="frequency6" value=""
                                                                                onclick="handleCheckbox(this)"
                                                                                <%=BindFrequency[5]=='1' ? 'checked' :''
