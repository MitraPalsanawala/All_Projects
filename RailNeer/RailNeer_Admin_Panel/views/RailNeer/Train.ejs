                function BindTrainNumber() {
                    var train_master_name =
                        document.getElementById("train_master_name").options[document.getElementById("train_master_name").selectedIndex].text;
                    const trainName = train_master_name.split('(')[0].trim();
                    var train_master_id = $("#train_master_name").val();
                    $.ajax({
                        async: false,
                        url: '/BindTrainNumber',
                        type: 'POST',
                        data: { train_master_name: trainName, train_master_id: train_master_id },
                        success: function (res) {
                            if (res.data.data.length > 0) {
                                $("#train_number").val(res.data.data[0].train_master_number)
                                $("#train_master_id").val(res.data.data[0].train_master_id)
                                $("#train_name").val(res.data.data[0].train_master_name)
                            } else {
                                $("#train_number").val('')
                                $("#train_master_id").val('')
                                $("#train_name").val('')
                            }
                        },
                    })
                }


