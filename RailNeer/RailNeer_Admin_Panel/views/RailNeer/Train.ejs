                                                                                <%=BindFrequency[1]=='1' ? 'checked' :''
                                                                                %>>
                                                                            <label for="frequency2">Tuesday</label>


                                                                            <input type="checkbox" id="frequency3"
                                                                                name="frequency3" value=""
                                                                                onclick="handleCheckbox(this)"
                                                                                <%=BindFrequency[2]=='1' ? 'checked' :''
                                                                                %>>
                                                                            <label for="frequency3">Wednesday</label>

                                                                            <input type="checkbox" id="frequency4"
                                                                                name="frequency4" value=""
                                                                                onclick="handleCheckbox(this)"
                                                                                <%=BindFrequency[3]=='1' ? 'checked' :''
                                                                                %>>
                                                                            <label for="frequency4">Thursday</label>

                                                                            <input type="checkbox" id="frequency5"
                                                                                name="frequency5" value=""
                                                                                onclick="handleCheckbox(this)"
                                                                                <%=BindFrequency[4]=='1' ? 'checked' :''
